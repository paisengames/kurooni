<?xml version='1.0' encoding='utf-8'?>
<widget id="paisen" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>kurooni</name>
    
    <config-file target="*-Info.plist" parent="UIApplicationExitsOnSuspend">
      <false/>
    </config-file>
    
    
    <preference name="BackupWebStorage" value="local" />
    
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="orientation" value="landscape" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-admob-free" spec="~0.27.0">
        <variable name="ADMOB_APP_ID" value="ca-app-pub-4799738082689694~3077688664" />
    </plugin>
    <plugin name="cordova-plugin-ionic-webview" spec="~4.2.0" />
</widget>
